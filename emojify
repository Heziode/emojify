);

# Remove duplicate items from an array
sub uniq {
    my %seen;
    grep !$seen{$_}++, @_;
}

# Gets emoji from the associative array
# 1 arg, must be a key
# Returns raw emoji character or empty string
sub get_emoji_if_exist {
    my $n = scalar (@_);

    if (($n == 1)) {
        my $result = shift;
        if (exists ($data{$result})) {
            $result = $data{$result};
        }
        return $result;
    }
    return "";
}

# Gets emoji from the associative array
# Returns raw emoji character or word
sub to_emoji {
    my $n = scalar (@_);
    my $result = shift;
    my @match;

    if ($n == 1) {
        if ((@match) = ($result =~ m/(\:[a-z0-9_]+\:)/g)) {
            @match = uniq (@match);

            foreach my $key (@match) {
                $result =~ s/$key/get_emoji_if_exist ($key)/eg;
            }
        }
    }

    return $result;
}

# Show usage
sub help {
    my $usage = <<EOF;
Emojify - emoji on the command line 

\tUSAGE: emojify [-l|--list] [-v|--version] [-h|--help] TEXT
EOF

    print (encode ('UTF-8', $usage));
}

# Show list of emoji
sub list {
    for (keys %data) {
        print (encode ('UTF-8', "$_ $data{$_}\n"));
    }
}

# Show version
sub version {
    print ("Emojify version $VERSION\n");
}

########
# MAIN #
########

if (-t STDIN) {
    my $cloption_help    = 0;
    my $cloption_list    = 0;
    my $cloption_version = 0;

    my ($cl_arg) = join (' ', @ARGV);
    my $nb_arg = scalar (@ARGV);

    GetOptions ("list|l" => \$cloption_list,
        "help|h"         => \$cloption_help,
        "version|v"      => \$cloption_version);

    if (($cloption_help == 1) or ($nb_arg == 0)) {
        help;
    }
    elsif ($cloption_list == 1) {
        list
    }
    elsif ($cloption_version == 1) {
        version;
    }
    else {
        print (encode ('UTF-8', to_emoji (decode ('UTF-8', $cl_arg))) . "\n");
    }
}
else {
    while (my $line = <STDIN>) {
        print (encode ('UTF-8', to_emoji (decode ('UTF-8', $line))));
    }
}
